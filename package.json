{
  "name": "devstack",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "backbone": "1.2.2",
    "backbone-crux": "git+https://bitbucket.org/codeyellow/backbone-crux.git#v2.3.0",
    "backbone.babysitter": "0.1.9",
    "backbone.marionette": "2.4.2",
    "backbone.paginator": "2.0.2",
    "backbone.wreqr": "1.3.4",
    "backbone.stickit": "0.9.2",
    "lodash": "3.10.1",
    "jquery": "2.1.4",
    "form-serializer": "2.5.0",
    "moment": "2.10.6",
    "marionette-crux-paginator": "git+https://bitbucket.org/codeyellow/marionette-crux-paginator.git#v0.2.0",
    "jquery-csrf-token": "git+https://bitbucket.org/codeyellow/jquery-csrf-token.git#v0.3.0"
  },
  "devDependencies": {
    "babel-core": "5.8.22",
    "babel-loader": "5.3.2",
    "css-loader": "0.16.0",
    "postcss-loader": "0.6.0",
    "sass-loader": "2.0.1",
    "file-loader": "0.8.4",
    "underscore-template-loader": "0.5.1",
    "extract-text-webpack-plugin": "0.8.2",
    "html-webpack-plugin": "git://github.com/ampedandwired/html-webpack-plugin.git#feature/loaders",
    "codeyellow-style": "git+https://bitbucket.org/codeyellow/codeyellow-style.git#v0.1.2",
    "dotenv": "1.2.0",
    "node-sass": "3.2.0",
    "mocha": "2.2.5",
    "chai": "3.2.0",
    "sinon": "git://github.com/uberVU/Sinon.JS.git#0aaf834c9f8ee302874a60b1aff69090896faf5c",
    "mocha-loader": "0.7.1",
    "run-headless-chromium": "0.0.12",
    "autoprefixer-core": "5.2.1",
    "webpack": "1.11.0",
    "webpack-dev-server": "1.10.1",
    "eslint": "1.2.1"
  },
  "engines": {
    "node": ">=0.12.5",
    "npm": ">= 2.11.0"
  },
  "scripts": {
    "pretest": "npm run -s build-js",
    "test": "npm run -s lint && run-headless-chromium test/index.html --disable-setuid-sandbox",
    "lint": "eslint .",
    "build": "npm run -s lint && npm run -s clean && npm run -s build-js",
    "build-js": "webpack --colors --progress --bail",
    "clean": "rm -rf dist/*",
    "start": ". .env; webpack-dev-server --output-public-path / --watch-poll --port $CY_WEBPACK_PORT --host $CY_WEBPACK_HOST"
  }
}
